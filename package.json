{
  "name": "leasing-system",
  "version": "1.0.0",
  "scripts": {
    "watch": "tsc -w",
    "prestart": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.json && tsc-alias && xcopy functions dist\\functions /E /I /Y && copy host.json dist\\ && copy local.settings.json dist\\",
    "start": "func start --javascript --script-root dist\\functions --settings local.settings.json",
    "test": "jest",
    "test:resetdb": "prisma migrate reset --force",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "prisma": "^4.16.2"
  },
  "devDependencies": {
    "@azure/functions": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "14.x",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^6.0.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^4.0.0",
    "zod": "^3.25.76"
  },
  "volta": {
    "node": "14.21.3"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
