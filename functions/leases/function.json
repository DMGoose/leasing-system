{
    "scriptFile": "index.js",
    "entryPoint": "default",
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "get",
                "post"
            ],
            "route": "leases/{id?}"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        }
    ]
}